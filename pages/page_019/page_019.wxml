<view class='style1'>
 <view wx:if="{{speechFlag==false}}" class='style2'>
   <image class='style3' src="{{speechImg}}"></image>
 </view>


 <view wx:if="{{cflag}}" class='style5'>
     <view class='style6'>
      <view bindtap='toCorrect' class='style8'>
        <image class='style10' src='../image/tabbar/22.png'></image>
      </view>
      <view bindtap='toError' class='style9'>
        <image class='style10' src='../image/tabbar/23.png'></image>
      </view>
     </view>
     
     <view animation="{{animationData1}}" catchtouchstart='toMoveStart' catchtouchend='toMoveEnd' class='style7'>
      <image catchtouchstart='toMoveStart' catchtouchend='toMoveEnd' class='style4' src="{{cimgs[imgindex].url}}"></image>
     </view>
  </view>

  <view class='style12'>
    <icon wx:if="{{resRigFlag}}" type="success" size="100" color="green"/>
    <icon wx:if="{{resErrFlag}}" type="clear" size="100" color="red"/>
  </view>
   
 
</view>


<!--底部导航条-->
<view class="footerstyle1">
    <view bindtap='toIndex' class="smallgroup">
      <image bindtap='toIndex' class='tabbar1' src='{{indeximg}}'></image>
      <view bindtap='toIndex' class='tabbar2'>首页</view>
    </view>
    <view bindtap='toPrevious' class="smallgroup">
      <image bindtap='toPrevious' class='tabbar1' src='{{previousImg}}'></image>
      <view bindtap='toPrevious' class='tabbar2'>上级</view>
    </view>
</view> 
