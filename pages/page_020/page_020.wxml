<view class='style1'>

  <view class='style2'>
    <image class='style3' src="{{backImg[backImgIndex].url}}"></image>
  </view>

  <view class='style6'>
      <view bindtap='speech' class="{{speechFlag!=true?'style7':'style11'}}">
        <image bindtap='speech' class="{{speechFlag!=true?'style12':'style4'}}" src='{{speechImg}}'></image>
      </view>

      <view wx:if="{{speechFlag}}" class='style8'>
        <image class='style5' src='../image/tabbar/15.png'></image>
      </view>
      
  </view>

</view>


<!--底部导航条-->
<view class="footerstyle1">
    <view bindtap='toIndex' class="smallgroup">
      <image bindtap='toIndex' class='tabbar1' src='{{indeximg}}'></image>
      <view bindtap='toIndex' class='tabbar2'>首页</view>
    </view>
    <view bindtap='toPrevious' class="smallgroup">
      <image bindtap='toPrevious' class='tabbar1' src='{{previousImg}}'></image>
      <view bindtap='toPrevious' class='tabbar2'>上级</view>
    </view>
</view> 
