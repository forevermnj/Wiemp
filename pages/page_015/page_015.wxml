<view class='style1'>
  <view class='style2'>
    <image class='style3' src='https://www.learnzp.com/Emp/mobile/page_015/1.png'></image>
    <image class='style3' src='https://www.learnzp.com/Emp/mobile/page_015/2.png'></image>
  </view>
  <view class='style4'>
       <image class='style5' src='../image/tabbar/14.png'></image>
  </view>
  <view class='style6'>
    <view wx:if="{{flag2==false}}" class='style7'><text class='style8'>A</text></view>
    <view bindtap='toRight' class="{{flag2?'style9':'style7'}}"><text class='style8'>B</text></view>
    <view wx:if="{{flag2==false}}" class='style7'><text class='style8'>C</text></view>
  </view>
</view>

<!--底部导航条-->
<view class="footerstyle1">
    <view bindtap='toIndex' class="smallgroup">
      <image bindtap='toIndex' class='tabbar1' src='{{indeximg}}'></image>
      <view bindtap='toIndex' class='tabbar2'>首页</view>
    </view>
    <view bindtap='toPrevious' class="smallgroup">
      <image bindtap='toPrevious' class='tabbar1' src='{{previousImg}}'></image>
      <view bindtap='toPrevious' class='tabbar2'>上级</view>
    </view>
</view>