.mainpagetop {
  height: 220px;
  background-color: rgb(67, 207, 124);
  text-align: center;
  color: white;
  display: flex;
  flex-direction: column;
  font-size: 18px;
}

#topmargin {
  margin-top: 30px;
}

.style1 {
  font-size: 14px;
}

.style3 {
  font-size: 60px;
}

.button1 {
  background: transparent;
  width: 200px;
  border-style: solid;
  border-width: 1px;
  border-color: white;
  -webkit-border-radius: 22px;
  -moz-border-radius: 22px;
  border-radius: 22px;
  color: white;
}

.link1 {
  font-size: 18px;
  padding-left: 20px;
  padding-top: 5px;
  padding-bottom: 5px;
  border-bottom: 1px solid lightgray;
}

.image1 {
  left: 25px;
  width: 48px;
  height: 48px;
  vertical-align: middle;
}

.arrow {
  position: absolute;
  right: 25px;
  padding-top: 10px;
}
