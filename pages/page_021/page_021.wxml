<view class='style1'>

  <scroll-view class='style2' scroll-y='true'>
   
    <block wx:for="{{resdata}}">
     <view data-hi="{{item.num}}" catchtap='toClickImg' class='style3'>
      <view data-hi="{{item.num}}" catchtap='toClickImg' class='style4'>
       <image class='style11' data-hi="{{item.num}}" catchtap='toClickImg' src="{{item.pic}}"></image>
      </view>
      <view data-hi="{{item.num}}" catchtap='toClickImg' class='style5'>
        <view class='style7'>{{item.tit}}</view>
        <view class='style6'>
         <progress radius="10" activeColor="#53CB14" percent="{{item.process}}" stroke-width="10"/>
        </view>
        <view class='style8'>{{item.tip}}</view>
        <view class='style9'>
         <image class='style10' src='../image/tabbar/26.png'></image>
         <text class='style12'>{{item.tt}}</text>
        </view>
      </view>
      
     </view>
    </block>
  
  </scroll-view>


</view>


<!--底部导航条-->
<view class="footerstyle1">
    <view bindtap='toIndex' class="smallgroup">
      <image bindtap='toIndex' class='tabbar1' src='{{indeximg}}'></image>
      <view bindtap='toIndex' class='tabbar2'>首页</view>
    </view>
    <view bindtap='toPrevious' class="smallgroup">
      <image bindtap='toPrevious' class='tabbar1' src='{{previousImg}}'></image>
      <view bindtap='toPrevious' class='tabbar2'>上级</view>
    </view>
</view> 
